{
  "name": "harmony/core-bundle",
  "description": "HarmonyCMS core bundle",
  "homepage": "https://projectharmony.github.io",
  "type": "symfony-bundle",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "David Sanchez",
      "email": "david38sanchez@gmail.com",
      "homepage": "http://davidsanchez.me",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "rollerworks/route-autowiring-bundle": "^1.1",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/asset": "^4.1",
    "symfony/console": "^4.1",
    "symfony/expression-language": "^4.1",
    "symfony/form": "^4.1",
    "symfony/framework-bundle": "^4.1",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": ^1.0",
    "symfony/process": "^4.1",
    "symfony/security-bundle": "^4.1",
    "symfony/serializer-pack": "^4.1",
    "symfony/swiftmailer-bundle": "^3.2",
    "symfony/twig-bundle": "^4.1",
    "symfony/validator": "^4.1",
    "symfony/web-link": "^4.1",
    "symfony/yaml": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "Harmony\\Bundle\\CoreBundle\\": ""
    }
  },
  "config": {
    "sort-packages": true
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "symfony": {
      "allow-contrib": false,
      "require": "~4.1."
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  }
}
